<h2> <%= @course.name %> </h2>

<h2> Sessions </h2>
<%= link_to 'New Session', new_session_path %>
<% @sessions.each do |session| %>
<p> <%= link_to session.name, session_path(session) %> | <%= session.questions.count %> questions</p>
<% end %>

<h2> Students </h2>
<%= link_to 'New Student', new_student_path %>
<% @students.each do |student| %>
<p> <%= student.name %> </p>
<% end %>
